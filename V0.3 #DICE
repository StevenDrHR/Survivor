        elif(rolledDice) and P4 == True:
            for x in range(0,outcome):
                if l3.Name == "Pos21":
                    l3 = PlayerPosList3
                l3 = l3.tail
            if l3.Name == "Pos1" or l3.Name == "Pos11" or l3.Name == "Pos21" or l3.Name == "Pos31" :
                    #Player3Class.Jet_Ri(outcome)
