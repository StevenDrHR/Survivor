        deltaTime = (ticks - getTicksLastFrame) / 1000.0
        getTicksLastFrame = ticks
        if(0.0 < diceDisplay):
            screen.blit(Dice[outcome-1],(800,100))
        if(0.0 < currentCardView):
            screen.blit(currentview,(400,100))
        rolledDice = False
        currentCardView -= deltaTime
        diceDisplay -= deltaTime
        pygame.display.update()

def RollDice():
   return random.randint(1,6)
def MainMenu2(crash,Exit,ManualButton, QuitButton,StartButton,event,mousex,mousey,NoButton,YesButton):
    crash = False
    xys = ((width * 0.5),(height * 0.10)) 
    xym = ((width * 0.5),(height * 0.4))
    xyq = ((width * 0.5),(height * 0.7)) 
    xyw = ((width * 0),(height * 0)) 
    xyl = ((width * 0.8),(height * 0.8))
    xye = ((width * 0.3),(height * 0.3))
    xyy = ((width * 0.51),(height * 0.52))
    xyn = ((width * 0.32),(height * 0.52))

    while not crash:
        for event in pygame.event.get():
            if event.type == MOUSEMOTION:
                mousex, mousey = pygame.mouse.get_pos()
            if event.type == pygame.KEYDOWN:
                if event.key == pygame.K_ESCAPE:
                    crash = True
            if event.type == pygame.QUIT:
                crash = True
            elif event.type == pygame.KEYDOWN and event.key == pygame.K_ESCAPE:
                crash = True
        else:
            if crash == False:
                wall= Image(Wallpaper,(xyw))
                Quit= Image(Quitbutton,(xyq))
                Startl= Image(Startbutton,(xys))
                Manual= Image(Manualbutton,(xym))
                ExitMenu = Image(ExitMessage,(xye))
                Yes = Image(Yesbutton,(xyy))
                No = Image(Nobutton,(xyn))

                #exit hoover
                if event.type == MOUSEMOTION and mousex > 352 and mousex < 552  and mousey > 312 and mousey < 421:
                        screen.blit(pygame.image.load("Nobutton2.png"),(xyn))
                else:
                        NoButton = Nob
                if event.type == MOUSEMOTION and mousex > 561 and mousex < 752  and mousey > 312 and mousey < 421:
                        screen.blit(pygame.image.load("Yesbutton2.png"),(xyy))
                else:
                        YesButton = Yesb
                    
                #ExitMenu interactie
                if event.type == MOUSEBUTTONUP and mousex > 352 and mousex < 552  and mousey > 312 and mousey < 421:
                    MainMenu(crash,Exit,ManualButton, QuitButton,StartButton,event,mousex,mousey) #no
                if event.type == MOUSEBUTTONUP and mousex > 561 and mousex < 752  and mousey > 312 and mousey < 421:
                    pygame.quit() #yes
                    quit()
                    
                        
        pygame.display.update()
    clock.tick(60)
class Player_1:
    def __init__(self,Name,Levenspunten):
        self.Name = Name
        self.Levenspunten = Levenspunten
    def Terry_Crews(Dice):
        if Dice == 1 or Dice == 6:
            Player_1.Levenspunten -= 10
        elif Dice == 2 or Dice == 5:
            Player_1.Levenspunten -= 15
        elif Dice == 3:
            Player_1.Levenspunten -= 25
        elif Dice == 4:
            Player_1.Levenspunten -= 20
        elif Dice == 5:
            Player_1.Levenspunten -= 23
        elif Dice == 6:
            Player_1.Levenspunten -= 26
        return K4
    def Jason_Statham(Dice):
        if Dice == 1: 
            Player_1.Levenspunten -= 15
        elif Dice == 2:
            Player_1.Levenspunten -= 17
        elif Dice == 3:
            Player_1.Levenspunten -= 19
        elif Dice == 4:
            Player_1.Levenspunten -= 21
        elif Dice == 5:
            Player_1.Levenspunten -= 23
        elif Dice == 6:
            Player_1.Levenspunten -= 26
        return K15
    def Wesley_Sniper(Dice):
        if Dice == 1:
            Player_1.Levenspunten -= 10
        elif Dice == 2 or Dice == 6:
            Player_1.Levenspunten -= 12
        elif Dice == 3 or Dice == 5:
            Player_1.Levenspunten -= 14
        elif Dice == 4:
            Player_1.Levenspunten -= 16
        return K14
    def Jet_Ri(Dice):
        if Dice == 1: 
            Player_1.Levenspunten -= 10
        elif Dice == 2:
            Player_1.Levenspunten -= 30
        elif Dice == 3:
            Player_1.Levenspunten -= 12
        elif Dice == 4:
            Player_1.Levenspunten -= 25
        elif Dice == 5:
            Player_1.Levenspunten -= 14
        elif Dice == 6:
            Player_1.Levenspunten -= 23
        return K11
    def Steve_Seagal(Dice):
        if Dice == 1: 
            Player_1.Levenspunten -= 10
        elif Dice == 2:
            Player_1.Levenspunten -= 15
        elif Dice == 3:
            Player_1.Levenspunten -= 12
        elif Dice == 4:
            Player_1.Levenspunten -= 11
        elif Dice == 5:
            Player_1.Levenspunten -= 25
        elif Dice == 6:
            Player_1.Levenspunten -= 20
        return K9
    def Super_Merio(Dice):
        if Dice == 1 or Dice == 2:
            Player_1.Levenspunten -= 10
        elif Dice == 3 or Dice == 4:
            Player_1.Levenspunten -= 30
        elif Dice == 5 or Dice == 6:
            Player_1.Levenspunten -= 15
        return K10
    def Vin_Dieser(Dice):
        if Dice == 1 or Dice == 5:
            Player_1.Levenspunten -= 15
        elif Dice == 2 or Dice == 4:
            Player_1.Levenspunten -= 25
        elif Dice == 3:
            Player_1.Levenspunten -= 30
        elif Dice == 6:
            Player_1.Levenspunten -= 15
        return K13
    def Chack_Norris(Dice):
        if Dice == 1: 
            Player_1.Levenspunten -= 15
        elif Dice == 2:
            Player_1.Levenspunten -= 28
        elif Dice == 3:
            Player_1.Levenspunten -= 27
        elif Dice == 4:
            Player_1.Levenspunten -= 25
        elif Dice == 5:
            Player_1.Levenspunten -= 29
        elif Dice == 6:
