            Player_4.Levenspunten -= 7
        elif Dice == 6:
            Player_4.Levenspunten -= 13
    def Jackie_Chen(Dice):
        if Dice == 1: 
            Player_4.Levenspunten -= 12
        elif Dice == 2:
            Player_4.Levenspunten -= 10
        elif Dice == 3:
            Player_4.Levenspunten -= 15
        elif Dice == 4:
            Player_4.Levenspunten -= 9
        elif Dice == 5:
            Player_4.Levenspunten -= 10
        elif Dice == 6:
            Player_4.Levenspunten -= 25
    def Bruce_Hee(Dice):
        if Dice == 1: 
            Player_4.Levenspunten -= 20
        elif Dice == 2:
            Player_4.Levenspunten -= 15
        elif Dice == 3:
            Player_4.Levenspunten -= 5
        elif Dice == 4:
            Player_4.Levenspunten -= 7
        elif Dice == 5:
            Player_4.Levenspunten -= 8
        elif Dice == 6:
            Player_4.Levenspunten -= 26

Player1Class = Player_1
Player1Class.Levenspunten = 100
Player1Class.Name = "Player 1 levenspunten:"

