import pygame

width = 1100
height = 600
size = ((width,height))
screen = pygame.display.set_mode(size)


class Empty:
    def __init__(self):
        self.isEmpty = True

Empty = Empty()

class Node:
    def __init__(self,Name,Position_X, Position_Y,Tail):
        self.isEmpty = False
        self.Name = Name
        self.Position_X = Position_X
        self.Position_Y = Position_Y
        self.tail = Tail

class Image():
    def __init__(self,image, x):
        self.Image = image
        self.pos = x
        screen.blit(image,(x))

