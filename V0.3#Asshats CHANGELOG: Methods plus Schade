class Player_1:
    def __init__(self,Name,Levenspunten):
        self.Name = Name
        self.Levenspunten = Levenspunten
    def Terry_Crews(Dice):
        if Dice == 1 or Dice == 6:
            Player_1.Levenspunten -= 10
        elif Dice == 2 or Dice == 5:
            Player_1.Levenspunten -= 15
        elif Dice == 3:
            Player_1.Levenspunten -= 25
        elif Dice == 4:
            Player_1.Levenspunten -= 20
    def Jason_Statham(Dice):
        if Dice == 1: 
            Player_1.Levenspunten -= 15
        elif Dice == 2:
            Player_1.Levenspunten -= 17
        elif Dice == 3:
            Player_1.Levenspunten -= 19
        elif Dice == 4:
            Player_1.Levenspunten -= 21
        elif Dice == 5:
            Player_1.Levenspunten -= 23
        elif Dice == 6:
            Player_1.Levenspunten -= 26
    def Wesley_Sniper(Dice):
        if Dice == 1:
            Player_1.Levenspunten -= 10
        elif Dice == 2 or Dice == 6:
            Player_1.Levenspunten -= 12
        elif Dice == 3 or Dice == 5:
            Player_1.Levenspunten -= 14
        elif Dice == 4:
            Player_1.Levenspunten -= 16
    def Jet_Ri(Dice):
        if Dice == 1: 
            Player_1.Levenspunten -= 10
        elif Dice == 2:
            Player_1.Levenspunten -= 30
        elif Dice == 3:
